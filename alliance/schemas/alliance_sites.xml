<table name="alliance_sites">
	<column name="site_srl" type="number" size="11" notnull="notnull" primary_key="primary_key" />
	<column name="domain" type="varchar" size="255" notnull="notnull" />
	<column name="type" type="char" size="1" notnull="notnull" />
	<column name="title" type="varchar" size="250" notnull="notnull" unique="unique_title" />
	<column name="status" type="char" size="20" notnull="notnull" />
	<column name="list_order" type="number" size="11" notnull="notnull" index="idx_list_order" />
	<column name="last_update" type="date" />
	<column name="regdate" type="date" />
</table>