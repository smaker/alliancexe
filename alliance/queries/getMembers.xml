<query id="getMembers" action="select">
	<tables>
		<table name="alliance_members" />
	</tables>
	<columns>
		<column name="site_srl" />
		<column name="target_srl" />
		<column name="member_id" />
	</columns>
	<conditions>
		<condition operation="equal" column="target_srl" var="member_srl" notnull="notnull" filter="number" />
	</conditions>
</query>